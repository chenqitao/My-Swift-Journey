{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.3",
  "body": [
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "width": "auto",
          "style": "emphasis",
          "items": [
            {
              "type": "Image",
              "url": "data:image/png;base64,"
            }
          ]
        },
        {
          "type": "Column",
          "width": "stretch",
          "style": "emphasis",
          "items": [
            {
              "type": "Image",
              "horizontalAlignment": "center",
              "url": "data:image/png;base64,"
            }
          ]
        },
        {
          "type": "Column",
          "width": "auto",
          "style": "emphasis",
          "items": [
            {
              "type": "Image",
              "url": "data:image/png;base64,"
            }
          ]
        }
      ]
    },
    {
      "type": "ColumnSet",
      "columns": [
        {
          "type": "Column",
          "width": "40px",
          "style": "emphasis",
          "items": [
            {
              "type": "Image",
              "url": "data:image/png;base64,"
            }
          ]
        },
        {
          "type": "Column",
          "width": 30,
          "style": "good",
          "items": [
            {
              "type": "TextBlock",
              "text": "30%"
            }
          ]
        },
        {
          "type": "Column",
          "width": 70,
          "style": "accent",
          "items": [
            {
              "type": "TextBlock",
              "text": "70%"
            }
          ]
        }
      ]
    }
  ]
}
