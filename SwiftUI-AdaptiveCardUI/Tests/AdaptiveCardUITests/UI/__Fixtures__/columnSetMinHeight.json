{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.3",
  "body": [
    {
      "type": "ColumnSet",
      "style": "emphasis",
      "minHeight": "25px",
      "columns": [
        {
          "type": "Column",
          "style": "default",
          "minHeight": "100px",
          "items": [
            {
              "type": "TextBlock",
              "wrap": true,
              "text": "Column minHeight > ColumnSet minHeight"
            }
          ],
          "width": "stretch"
        }
      ]
    },
    {
